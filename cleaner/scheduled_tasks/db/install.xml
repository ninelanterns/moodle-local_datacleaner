<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/datacleaner/cleaner/scheduled_tasks/db" VERSION="2019030400" COMMENT="XMLDB file for Moodle local/datacleaner/cleaner/scheduled_tasks"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="cleaner_scheduled_tasks" COMMENT="Scheduled tasks config - whether to disable the task in a clean">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="taskscheduledid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
